library(ggplot2)
library(dplyr)
data(diamonds)
summary(diamonds)
clarity_groups <- group_by(diamonds, clarity)
diamondsByClarity <- diamonds %>%
group_by(clarity) %>%
summarise(mean_price = mean(price),
median_price = median(price),
min_price = min(price),
max_price = max(price),
n = n()) %>%
arrange(clarity)
diamondsByClarity <- diamonds %>%
group_by(clarity) %>%
summarise(mean_price = mean(price),
median_price = median(price)*1.,
min_price = min(price),
max_price = max(price),
n = n()) %>%
arrange(clarity)
